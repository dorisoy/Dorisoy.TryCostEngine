<MudGrid Class="docs-footer">
    <MudItem xs="12">
        <MudToolBar DisableGutters="true" Dense="true">
            <MudText Typo="Typo.body1">Copyright Â© @DateTime.Now.Year TPM</MudText>
        </MudToolBar>
    </MudItem>
</MudGrid>

@code {
    string MudBlazorVersion
    {
        get
        {
            var v = typeof(MudText).Assembly.GetName().Version;
            return $"v{v.Major}.{v.Minor}.{v.Build}.";
        }
    }
}